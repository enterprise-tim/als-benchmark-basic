{
  "name": "async-node-stats",
  "version": "1.0.0",
  "description": "Performance benchmarks for AsyncLocalStorage overhead analysis across Node.js versions",
  "type": "module",
  "scripts": {
    "dev": "./copy-results-for-dev.sh && vite",
    "dev-cleanup": "vite && ./cleanup-dev-results.sh",
    "dev-stop": "./cleanup-dev-results.sh",
    "build": "vite build && mkdir -p dist/results && cp -r results/* dist/results/ 2>/dev/null || true && cp -r public/* dist/ 2>/dev/null || true && ./cleanup-dev-results.sh",
    "preview": "./copy-results-for-dev.sh && vite preview",
    "preview-cleanup": "vite preview && ./cleanup-dev-results.sh",
    "deploy": "gh-pages -d dist",
    "start": "node src/index.js",
    "benchmark": "node --max-old-space-size=8192 --expose-gc src/benchmark.js",
    "memory-test": "node --max-old-space-size=8192 --expose-gc --trace-gc src/memory-test.js",
    "benchmark-optimized": "node --max-old-space-size=8192 --expose-gc --no-compilation-cache --predictable --single-threaded-gc src/benchmark.js",
    "memory-test-verbose": "node --max-old-space-size=8192 --expose-gc --trace-gc --trace-gc-verbose src/memory-test.js",
    "test-all": "npm run benchmark && npm run memory-test",
    "test-all-optimized": "npm run benchmark-optimized && npm run memory-test-verbose",
    "generate-report": "node src/report-generator.js",
    "compare-versions": "node src/version-comparison.js",
    "multi-iteration": "node scripts/run-multi-iterations.js",
    "all-versions": "node scripts/run-all-versions.js",
    "test-setup": "node scripts/test-setup.js",
    "serve-docs": "npx http-server docs -p 8080",
    "validate-versions": "node scripts/validate-versions.js",
    "get-versions": "node scripts/get-node-versions.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-results": "./copy-results-for-dev.sh",
    "cleanup-results": "./cleanup-dev-results.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "benchmark": "^2.1.4",
    "chart.js": "^4.5.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "gh-pages": "^6.1.0",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "asynclocalstorage",
    "performance",
    "nodejs",
    "benchmark",
    "overhead"
  ],
  "author": "Tim O'Brien",
  "license": "MIT"
}
